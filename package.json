{
  "name": "eslint-plugin-hardsort",
  "version": "0.0.1",
  "description": "Sorting is attempted under limited conditions.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "hardsort",
    "strong-alignment"
  ],
  "author": "sotaneum",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "prepack": "npmignore --auto --commentLines=autogenerated",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "npmignore": "^0.3.1",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-hardsort": "file:",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "MIT",
  "publishConfig": {
    "ignore": [
      ".github/",
      "!lib",
      "docs/",
      "tests/",
      "*.md",
      ".eslintrc"
    ]
  }
}
